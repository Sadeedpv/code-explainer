{
  "name": "code-explainer",
  "displayName": "Code Explainer",
  "description": "Explains every bit of code inside your editor",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "activationEvents": [
    "onCommand:code-explainer.hello-world",
    "onCommand:code-explainer.explain"
  ],
  "contributes": {
    "commands": [
      {
        "command": "code-explainer.hello-world",
        "title": "Hello World!"
      },
      {
        "command": "code-explainer.explain",
        "title": "Explain",
        "category": "View"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.81.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
